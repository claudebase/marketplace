{
  "$schema": "https://json.schemastore.org/claude-settings",
  "_description": "Unix (macOS/Linux) settings for Developer Kit plugin",
  "_instructions": [
    "Copy this file to ~/.claude/settings.json",
    "Replace placeholder values with your actual API keys",
    "Restart Claude Code after making changes"
  ],
  "enabledPlugins": {
    "developer-kit@claudebase": true
  },
  "env": {
    "TAVILY_API_KEY": "your-actual-tavily-api-key",
    "GITHUB_TOKEN": "your-actual-github-token",
    "ENABLE_SECURITY_REMINDER": "1",
    "ENABLE_FORMAT_ON_SAVE": "1"
  },
  "terminal": {
    "shell": "/bin/zsh",
    "shellArgs": ["-l"]
  },
  "mcp": {
    "timeout": 60000
  }
}
